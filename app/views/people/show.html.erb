<h1>People#show</h1>
<p>Find me in app/views/people/show.html.erb</p>

<div class="container">
  <p>birthday:<%= @person.birthday %></p>
  <p>age:<%= @person.age %></p>
  <p>full retirement age:<%= @person.full_retirement_age %></p>
  <p>sex:<%= @person.sex %></p>
  <p>current income:<%= number_to_currency(@person.current_income) %></p>
  <p>life expectancy:<%= @person.life_expectancy %></p>
  <p>claim date:<%= @person.claim_date %></p>
  <p>spousal benefits:<%= @person.spousal_benefits %></p>
